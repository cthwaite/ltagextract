(ROOT
 (S=H
  (NP
   (NP=H (DT The) (NN=H function))
   (PP (IN=H of) (NP (DT a) (NN=H (JJ double) (NN=H bond)))))
  (VP=H
   (VBZ=H is)
   (S
    (VP=H
     (TO=H to)
     (VP
      (VB=H hold)
      (PRT (RP=H together))
      (NP
       (NP=H (CD two) (NNS=H oxygens))
       (CC and)
       (NP (DT a) (NN=H carbon)))))))
  (. .)))
